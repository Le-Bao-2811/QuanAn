@model QuanAnGiaDinh.Web.ViewModels.DatBanVM
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="booking">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-lg-7">
				<div class="booking-content">
					<div class="section-header">
						<p>Đặt bàn trước</p>
						<h2>Đặt bàn trước để trãi nghiệm không gian lãn mạn tại quán</h2>
					</div>
					<div class="about-text">
						<p>
							Không gian quán được thết kế sang trọng, rộng rãi, có nhiều không gian đẹp để quý khách hàng có thể cho ra những tấm ảnh với khonong gian đẹp lung linh. 
						</p>
						<p>
							Quán được chưa làm 3 khu vực: Sảnh A với sức chứa là 30 bàn. Phòng tiệc có 20 phòng, và khu vực Sân thượng với không gian quay ra bãi biển
						</p>
					</div>
				</div>
			</div>
			@*đặt bàn*@
			<div class="col-lg-5">
				<div class="booking-form">
					<form id="form-datban" >
						<div class="control-group">
							<div class="input-group">
								<input asp-for="TenKH" type="text" class="form-control" placeholder="Họ và tên" required="required" />
								<div class="input-group-append">
									<div class="input-group-text"><i class="far fa-user"></i></div>
								</div>
							</div>
						</div>
						<div class="control-group">
							<div class="input-group">
								<input asp-for="STD" type="text" class="form-control" placeholder="Mobile" required="required" />
								<div class="input-group-append">
									<div class="input-group-text"><i class="fa fa-mobile-alt"></i></div>
								</div>
							</div>
						</div>
						<div class="control-group">
							<div class="input-group">
								<select asp-for="KhuVuc" class="custom-select form-control">
									<option style="background-color:black;color:white" value="Sảnh A">Sảnh A</option>
									<option style="background-color:black;color:white" value="Phòng tiệc">Phòng tiệc</option>
									<option style="background-color:black;color:white" value="Sân thượng">Sân thương</option>
									
								</select>
								<div class="input-group-append">
									<div class="input-group-text"><i class="fa fa-chevron-down"></i></div>
								</div>
							</div>
						</div>
						<div class="control-group">
							<div class="input-group date" id="date" data-target-input="nearest">
								<input type="text" asp-for="Ngay" class="form-control datetimepicker-input" placeholder="Date" data-target="#date" data-toggle="datetimepicker" />
								<div class="input-group-append" data-target="#date" data-toggle="datetimepicker">
									<div class="input-group-text"><i class="far fa-calendar-alt"></i></div>
								</div>
							</div>
						</div>
						<div class="control-group">
							<div class="input-group time" id="time" data-target-input="nearest">
								<input asp-for="Gio" type="text" class="form-control datetimepicker-input" placeholder="Time" data-target="#time" data-toggle="datetimepicker" />
								<div class="input-group-append" data-target="#time" data-toggle="datetimepicker">
									<div class="input-group-text"><i class="far fa-clock"></i></div>
								</div>
							</div>
						</div>

						<div class="justify-content-center">
							<a class="btn btn-light datban" >Book Now</a>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- Booking End -->
<!-- About Start -->
<div class="about">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-lg-6">
				<div class="about-img">
					<img src="data:image/jpeg;base64," alt="Image">
				</div>
			</div>
			<div class="col-lg-6">
				<div class="about-content">
					<div class="section-header">
						<p>Hạo Gia Quán</p>
						<h2>Lịch sử nấu ăn 200 năm</h2>
					</div>
					<div class="about-text">
						<p>
							Vịt chạy đồng - cái cách chăn nuôi thú vị chỉ có ở mảnh đất miền Tây, nơi ruộng đồng nối liền kênh rạch, nơi người sống chan hòa, hào sảng giữa thiên nhiên. Cả vịt cả người sống cuộc sống du mục suốt ba tháng trời, khi đàn vịt đã phổng phao, mỡ màng nhờ ruộng đồng, cá tôm nuôi lớn. Không ăn vịt vào mùa này thì còn chờ đến bao giờ nữa.
						</p>
						<a class="btn custom-btn" href="">Book A Table</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- About End -->
<!-- Video Modal Start-->
<div class="modal fade" id="videoModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-body">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
				<!-- 16:9 aspect ratio -->
				<div class="embed-responsive embed-responsive-16by9">
					<iframe class="embed-responsive-item" src="" id="video" allowscriptaccess="always" allow="autoplay"></iframe>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- Video Modal End -->
<!-- Feature Start -->
<div class="feature">
	<div class="container">
		<div class="row">
			<div class="col-lg-5">
				<div class="section-header">
					<p>Thức ăn mang đi</p>
					<h2> phải đảm bảo 6 yếu tố</h2>
				</div>
				<div class="feature-text">
					<div class="feature-img">
						<div class="row">
							<div class="col-6">
								<img src="~/Layout/img/feature-1.jpg" alt="Image">
							</div>
							<div class="col-6">
								<img src="~/Layout/img/feature-2.jpg" alt="Image">
							</div>
							<div class="col-6">
								<img src="~/Layout/img/feature-3.jpg" alt="Image">
							</div>
							<div class="col-6">
								<img src="~/Layout/img/feature-4.jpg" alt="Image">
							</div>
						</div>
					</div>
					<p>
						Ngoài việc đến nhà hàng và thưởng thức các món ăn ngon, quý khách còn có thể thưởng thức các món ăn tại nhà hàng qua hình thức đặt trực tuyế
					</p>
					<a class="btn custom-btn" asp-action="MenuClient">Đặt hàng ngay</a>
				</div>
			</div>
			<div class="col-lg-7">
				<div class="row">
					<div class="col-sm-6">
						<div class="feature-item">
							<i class="flaticon-cooking"></i>
							<h3>Đầu bếp</h3>
							<p>
								là những người có kinh nghiệm trên 10 năm trong lĩnh vực làm bếp  
							</p>
						</div>
					</div>
					<div class="col-sm-6">
						<div class="feature-item">
							<i class="flaticon-vegetable"></i>
							<h3>Rau củ</h3>
							<p>
								Phải là nguyên liệu tươi mới và có nguồn gốc rõ ràng
							</p>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-sm-6">
						<div class="feature-item">
							<i class="flaticon-medal"></i>
							<h3>Chất lượng</h3>
							<p>
								Phải luôn bảo chất lượng món ăn theo quy định của luật an toàn thực phẩm
							</p>
						</div>
					</div>
					<div class="col-sm-6">
						<div class="feature-item">
							<i class="flaticon-meat"></i>
							<h3>Thịt</h3>
							<p>
								Phải là thịt tươi sống và có nguồn gốc rõ rang
							</p>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-sm-6">
						<div class="feature-item">
							<i class="flaticon-courier"></i>
							<h3>Giao hàng</h3>
							<p>
								Giao hàng nhanh trong khi thức ăn còn nóng
							</p>
						</div>
					</div>
					<div class="col-sm-6">
						<div class="feature-item">
							<i class="flaticon-fruits-and-vegetables"></i>
							<h3>Ăn toàn vệ sinh</h3>
							<p>
								Luôn đảm bảo vệ sinh an toàn thực phẩm trước khi đến tay người dùng
							</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- Feature End -->
<!-- Food Start -->
<div class="food">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-md-4">
				<div class="food-item">
					<i class="flaticon-burger"></i>
					<h2>Món ăn chính</h2>
					<p>
						Với những món ăn từ dân dã cho đến cao cấp, menu đa dạng với nhiều lựa chọn
					</p>
					<a href="">View Menu</a>
				</div>
			</div>
			<div class="col-md-4">
				<div class="food-item">
					<i class="flaticon-snack"></i>
					<h2>Thức ăn nha</h2>
					<p>
						Gồm nhiều món đang hot trên thị trường và được giới trẻ hết sức ưa chuộng
					</p>
					<a href="">View Menu</a>
				</div>
			</div>
			<div class="col-md-4">
				<div class="food-item">
					<i class="flaticon-cocktail"></i>
					<h2>Nước uống</h2>
					<p>
						Gồm nhiều loại khác nhau từ thức uống có cồn đến thức uống có ga
					</p>
					<a href="">View Menu</a>
				</div>
			</div>
		</div>
	</div>
</div>
<partial name="Cart" />

<div class="modal fade" id="modal-Sua" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body" id="modal-body">
				<partial name="_AddDatHang" />
			</div>

		</div>
	</div>
</div>
@section js{
	<script src="~/lib/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>
	<script src="~/Client/MenuClient.js" asp-append-version="true"></script>
	<script src="~/Client/Cart.js" asp-append-version="true"></script>
 }
